{
  "schemaVersion": 1,
  "id": "cloudsettings",
  "version": "${version}",

  "name": "CloudSettings",
  "description": "A simple Mod to sync your Minecraft settings between Minecraft Modpacks",
  "authors": [],
  "contact": {},

  "license": "MIT",
  "icon": "icon.png",

  "environment": "client",
  "entrypoints": {
    "client": [
      "de.blutmondgilde.cloudsettings.fabric.CloudSettingsFabric"
    ]
  },
  "mixins": [
    "CloudSettings.mixins.json",
    "CloudSettings-common.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.0",
    "minecraft": ">=1.16.5"
  }
}
